<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="ControlBackgroundBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#FF81D8D8" Offset="0"/>
        <GradientStop Color="Aquamarine" Offset="0.5"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ReadOnlyControlBackgroundBrush" Color="Transparent"/>

    <LinearGradientBrush x:Key="WindowBackgroundBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="Gray" Offset="0"/>
        <GradientStop Color="DarkGray" Offset="0.5"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ButtonBackgroundBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#FFB9B0B0" Offset="0"/>
        <GradientStop Color="LightGray" Offset="0.5"/>
    </LinearGradientBrush>

</ResourceDictionary>