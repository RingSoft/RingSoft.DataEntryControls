<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:RingSoft.DataEntryControls.WPF.DataEntryGrid"
                    xmlns:wpf="clr-namespace:RingSoft.DataEntryControls.WPF">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/RingSoft.DataEntryControls.WPF;component/Themes/CustomContentControlTemplate.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="{x:Type local:DataEntryGridButton}" BasedOn="{StaticResource {x:Type Button}}"/>

    <Style TargetType="{x:Type local:DataEntryGridCheckBox}" BasedOn="{StaticResource {x:Type CheckBox}}"/>

    <Style TargetType="{x:Type local:DataEntryGridCustomControl}" BasedOn="{StaticResource {x:Type wpf:CustomContentControl}}">
    </Style>

</ResourceDictionary>